sc_agent_of_finding_component_dependencies
=> nrel_main_idtf:
	[sc-агент назначения роли пользователю] (* <- lang_ru;; *);
	[sc-agent of assignment user role] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_component_dependencies_condition => ..sc_agent_of_finding_component_dependencies_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_find_component_dependencies
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_component_dependencies
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_component_dependencies	
		};;

	-> sc_agent_of_finding_component_dependencies_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_finding_component_dependencies_condition
= [*
	action_find_component_dependencies _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_finding_component_dependencies_result
= [*
	action_find_component_dependencies _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
